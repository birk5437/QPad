<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
     package="org.me.qpad"
     android:versionCode="6"
     android:versionName="1.3"
     >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-sdk
    android:minSdkVersion="4">
    </uses-sdk>
    <application android:icon="@drawable/qpad_icon" android:label="QPad">
        <activity android:name=".Menu" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity android:name=".Game"></activity>
	<activity android:name=".SubmitHighScore"></activity>
        <activity android:name=".HighScores"></activity>
    </application>
</manifest>
